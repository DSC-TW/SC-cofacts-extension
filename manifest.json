{
    "name": "confact extension",
    "description" : "Solution Challenge",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "contextMenus",
      "activeTab",
      "http://localhost/*",
      "https://us-central1-sc-cofacts.cloudfunctions.net/*",
      "storage",
      "notifications"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }